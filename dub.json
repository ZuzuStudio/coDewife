		{
			"name": "standalone",
			"targetType": "executable",
			"targetPath": "build/app",
			"workingDirectory": "build/app"
		},
		{
			"name": "intliteral",
			"sourcePath": "skapp/intliteral/test",
			"targetType": "executable",
			"targetName": "testIntliteral",
			"targetPath": "build/skapp",
			"workingDirectory": "build/skapp" 
		},
		{
			"name": "benchIntliteral",
			"sourcePath": "skapp/intliteral/benchmark",
			"targetType": "executable",
			"targetName": "benchIntliteral",
			"targetPath": "build/skapp",
			"workingDirectory": "build/skapp" 
		}
	]
}
